ALTER QMGR MONQ(LOW)
ALTER QMGR PERFMEV(ENABLED) CHLEV(ENABLED)
ALTER QMGR AUTHOREV(ENABLED) INHIBTEV(ENABLED) LOCALEV(ENABLED) REMOTEEV(ENABLED) STRSTPEV(ENABLED)
ALTER QMGR CHLEV(ENABLED) SSLEV(ENABLED) CHADEV(ENABLED)
ALTER QMGR STATQ(ON)
ALTER QMGR STATINT(900)
ALTER QMGR STATMQI(ON)

DEFINE CHANNEL(SICHL) CHLTYPE(SVRCONN) TRPTYPE(TCP) SSLCAUTH(OPTIONAL) SSLCIPH('ANY_TLS12_OR_HIGHER') REPLACE
SET CHLAUTH(SICHL) TYPE(BLOCKUSER) USERLIST('nobody') ACTION(ADD)

DEFINE CHANNEL(METRICS) CHLTYPE(SVRCONN) TRPTYPE(TCP) SSLCAUTH(OPTIONAL) REPLACE
SET CHLAUTH(METRICS) TYPE(BLOCKUSER) USERLIST('nobody') ACTION(ADD)